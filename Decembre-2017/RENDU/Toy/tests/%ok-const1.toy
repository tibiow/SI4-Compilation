const int x = 10;
const int y = 20, z = 30;

int main() {
    const bool b = false;
    const float pi = 3.14159;
    const string str = "Hello, world";
    int w = 100;

    print(x, y, z);
    print(x+1, y+1, z+1);
    print(b, pi, str);

    print((w + x + y + z == 160) ? "SUCCESS\n": "FAILURE");
    return 0;
}
