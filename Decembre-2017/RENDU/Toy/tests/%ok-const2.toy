const int x = 10;
const int y = 20;

int main() {
    const int x = 1;
    int total = 0;

    for (int x = 0; x < y; x++) {
        total = total + x;
    }
    print(x, " ", y, " ", total, "\n");
    print((x == 1 and y == 20 and total == 190) ? "SUCCESS\n": "FAILURE");
    return 0;
}
