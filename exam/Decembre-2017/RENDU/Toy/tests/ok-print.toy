class C {}
class D { void printobj() { print("[[ An object of class D ]]"); }}


int main() {
  print("A string: ", "abc", "\n");
  print("An int: ", 3, "\n");
  print("An expression: ", 3 * (2 + 4), "\n");
  print("A boolean False: ", false, " True: ", true, "\n");
  print("A boolean expression: ", (3 < 4) and (4 < 5), "\n");
  print("An instance of Object: ", new Object, "\n");
  print("An instance of class C: ", new C, "\n");
  print("An object whose class overloads the printobj method: ", new D, "\n");
  print("End of test: SUCCESS\n");
}
  

