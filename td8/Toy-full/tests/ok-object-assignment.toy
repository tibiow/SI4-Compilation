class C extends Object {}
class D extends C      {}

int main() {
    Object o1, o2;
    C c1, c2;
    D d1, d2;

    o1 = o2 = c1 = c2 = d1 = d2 = new D;
    print(o1, " ", o2, "\n", c1, " ", c2, "\n", d1, " ", d2, "\n");
    print("SUCCESS\n");
}
